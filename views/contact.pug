      header(id="fh5co-header" class="fh5co-cover fh5co-cover-sm" role="banner" style="background-image:url(images/img_bg_2.jpg);")
        div(class="overlay")
        div(class="container")
          div(class="row")
            div(class="col-md-8 col-md-offset-2 text-center")
              div(class="display-t")
                div(class="display-tc animate-box" data-animate-effect="fadeIn")
                  h1 Contact Us
                  h2 Free html5 templates Made by
                    a(href="http://freehtml5.co" target="_blank") freehtml5.co

    div(class="fh5co-section")
      div(class="container")
        div(class="row")
          div(class="col-md-6 animate-box")
            h3 Get In Touch
            form(action="#")
              div(class="row form-group")
                div(class="col-md-6")
                  // label(for="fname") First Name
                  input(type="text" id="fname" class="form-control" placeholder="Your firstname")
                
                div(class="col-md-6")
                  // label(for="lname") Last Name
                  input(type="text" id="lname" class="form-control" placeholder="Your lastname")

              div(class="row form-group")
                div(class="col-md-12")
                  // label(for="email") Email
                  input(type="text" id="email" class="form-control" placeholder="Your email address")

              div(class="row form-group")
                div(class="col-md-12")
                  // label(for="subject") Subject
                  input(type="text" id="subject" class="form-control" placeholder="Your subject of this message")

              div(class="row form-group")
                div(class="col-md-12")
                  // label(for="message") Message
                  textarea(name="message" id="message" cols="30" rows="10" class="form-control" placeholder="Say something about us")
                
              div(class="form-group")
                input(type="submit" value="Send Message" class="btn btn-primary")
          
          div(class="col-md-5 col-md-push-1 animate-box")
            
            div(class="fh5co-contact-info")
              h3 Contact Information
              ul 
                li(class="address") 198 West 21th Street, br  Suite 721 New York NY 10016
                li(class="phone")
                  a(href="tel://1234567920") +1235 2355 98
                li(class="email")
                  a(href="mailto:info@yoursite.com") info@yoursite.com
                li(class="url")
                  a(href="http://FreeHTML5.co") FreeHTML5.co

